# Copy to .env and fill values (do not commit .env)

# Local development
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-anon-key

# OAuth Providers (required for Google/Apple sign in)
# Google OAuth (create at: https://console.cloud.google.com/apis/credentials)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Apple Sign In (create at: https://developer.apple.com/developer/sign-in-with-apple/)
VITE_APPLE_CLIENT_ID=your-apple-client-id

# Optional: Sentry
VITE_SENTRY_DSN=
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# Optional: Analytics
# VITE_ANALYTICS_ENABLED=false
VITE_POSTHOG_KEY=
VITE_POSTHOG_HOST=

# Optional: Image generation (do not commit real tokens)
HUGGINGFACE_API_TOKEN=
HUGGINGFACE_MODEL=
STABLE_DIFFUSION_API_URL=
OPENAI_API_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Feature Flags (set to true to enable new features)
VITE_FEATURE_DAILY_QUEST_ENABLED=false
VITE_FEATURE_ART_GENERATION_ENABLED=false

# AI Integration (add these for AI features)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
STABILITY_API_KEY=your_stability_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# AI Configuration
VITE_FEATURE_AI_ENHANCED_PROMPTS=true
VITE_FEATURE_AI_IMAGE_ANALYSIS=true
VITE_FEATURE_AI_TAG_GENERATION=true
VITE_FEATURE_AI_MOOD_DETECTION=true
VITE_AI_MAX_REQUESTS_PER_HOUR=100
VITE_AI_CACHE_RESULTS=true
VITE_AI_DEFAULT_SERVICE=openai
VITE_AI_DEFAULT_MODEL=gpt-4
VITE_AI_DEFAULT_TEMPERATURE=0.7
VITE_AI_DEFAULT_MAX_TOKENS=2048
VITE_AI_COST_TRACKING_ENABLED=true
VITE_AI_BUDGET_LIMIT_PER_MONTH=50.00
